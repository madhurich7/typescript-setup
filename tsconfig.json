        "module": "commonjs",
        "rootDir": "./src",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": false,
        "outDir": "./dist",
        "noEmitOnError": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": true
